        android:textSize="40dp"
        android:textColor="@color/Black"
        android:fontFamily="@font/sf_pro"
        android:layout_marginTop="70dp"
        android:layout_marginLeft="18dp"

    />
    <TextView
        android:id="@+id/txtmsj"
